  "bugs": {
    "url": "https://github.com/timblack1/cidr-lib/issues"
  },
  "homepage": "https://github.com/timblack1/cidr-lib#readme",
  "devDependencies": {
    "@jedmao/semantic-release-npm-github-config": "^1.0.9",
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "mocha": "^6.2.2",
    "semantic-release": "^15.14.0",
    "watch": "^1.0.2"
  }
